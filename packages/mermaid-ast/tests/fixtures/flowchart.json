          "description": "Click href link"
        },
        {
          "name": "click_href_target",
          "input": "graph LR\n    A-->B\n    click A href \"https://example.com\" _blank",
          "description": "Click href with target"
        },
        {
          "name": "click_callback_tooltip",
          "input": "graph LR\n    A-->B\n    click A callback \"Tooltip text\"",
          "description": "Click callback with tooltip"
        }
      ]
    },
    "complex": {
      "description": "Complex diagrams",
      "fixtures": [
        {
          "name": "decision_flow",
          "input": "graph LR\n    A[Hard] -->|Text| B(Round)\n    B --> C{Decision}\n    C -->|One| D[Result 1]\n    C -->|Two| E[Result 2]",
          "description": "Decision flow chart"
        },
        {
          "name": "multiple_subgraphs",
          "input": "graph TB\n    subgraph one\n        a1-->a2\n    end\n    subgraph two\n        b1-->b2\n    end\n    a1-->b1",
          "description": "Multiple subgraphs with connections"
        },
        {
          "name": "full_example",
          "input": "flowchart TD\n    A[Start] --> B{Is it?}\n    B -->|Yes| C[OK]\n    C --> D[Rethink]\n    D --> B\n    B ---->|No| E[End]",
          "description": "Full example with loop"
